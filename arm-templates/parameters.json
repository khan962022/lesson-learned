        "nicDeleteOption": {
            "value": "Delete"
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "secureBoot": {
            "value": true
        },
        "vTPM": {
            "value": true
        },
        "bastionHostName": {
            "value": "Bastiion"
        },
        "publicIpAddressName": {
            "value": "Bastion-ip"
        },
        "BastionSubnetName": {
            "value": "AzureBastionSubnet"
        }
