        "bastionSubnetIpPrefix": {
            "value": [
                "10.0.1.0/27"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.0.0/24"
                    }
                }
            ]
        },
        "virtualMachineName": {
            "value": "web03"
